<?xml version="1.0"?>
<Template>
	<TemplateConfiguration>
		<_Name>Cocoa Window with Controller</_Name>
		<Icon>xib-template-window</Icon>
		<_Category>Xamarin.Mac</_Category>
		<LanguageName>F#</LanguageName>
		<ProjectType>XamMac2</ProjectType>
		<_Description>Creates an Cocoa Window with a Controller, for Xamarin.Mac.</_Description>
	</TemplateConfiguration>

	<TemplateFiles>
		<File name="${Name}.xib" src="WindowWithController/Window.xib" DefaultExtension=".xib" SuppressAutoOpen="True"/>
		<UnformattedFile name="${Name}.fs" src="WindowWithController/Window.fs"/>
		<UnformattedFile name="${Name}Controller.fs" src="WindowWithController/WindowController.fs"/>
	</TemplateFiles>
</Template>